{
  "expo": {
    "name": "My Pomodoro App",
    "slug": "My Pomodoro App",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "2.0",
    "orientation": "portrait",
    "icon": "./assets/myIcon.png",
    "splash": {
      "image": "./assets/mySplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#999999"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}